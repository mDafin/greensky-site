{
  "name": "greensky-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "video:build": "scripts/build-video.sh",
    "tunnel": "ngrok http 3000",
    "share": "npm-run-all -p dev tunnel",
    "url": "curl -s http://127.0.0.1:4040/api/tunnels | python -c 'import sys,json; d=sys.stdin.read(); print(next((x.get(\"public_url\",\"\") for x in (json.loads(d).get(\"tunnels\") or []) if x.get(\"proto\")==\"https\"), \"\")) if d.strip() else \"\"'"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.19",
    "next": "15.5.4",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^4.1.13",
    "typescript": "^5"
  }
}
